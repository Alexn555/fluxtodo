{
  "name": "flux_todo",
  "version": "1.0.0",
  "description": "A simple todo flux application",
  "main": "index.js",
  "scripts": {
    "server": "node index.js",
    "build:css": "gulp css-handle",
    "watch-css": "catw public/css/*.css -o public/css/bundle.css -v",
    "build-less": "lessc public/css/main.less public/css/main.css",
    "build:client": "webpack",
    "build-and-server": "webpack & npm run server",
    "build-and-prod": "webpack & npm run build:css & npm run server",
    "watch": "watchify -o public/bundle.js -v -d .",
    "start": "webpack-dev-server --inline --content-base ."
  },
  "author": "Aleksandr Kalinin",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "flux": "^2.0.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.1.3",
    "keymirror": "^0.1.1",
    "less": "^2.7.2",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-bootstrap-date-picker": "^5.0.1",
    "react-bootstrap-slider": "^1.1.5",
    "react-dom": "^15.3.0",
    "react-router": "^3.0.2",
    "reactstrap": "^4.3.0",
    "uglifyjs": "^2.4.10",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.24.0",
    "catw": "0.0.1",
    "envify": "~3.4.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.0.3",
    "gulp-concat": "^2.6.1",
    "jsdom": "^9.12.0",
    "react-addons-test-utils": "^15.4.2",
    "reactify": "^1.1",
    "run-sequence": "^1.2.2",
    "tap": "^0.4.13",
    "uglifycss": "0.0.25",
    "watchify": "~3.7.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
